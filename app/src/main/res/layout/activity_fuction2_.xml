<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="40dp"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="你喜欢吃什么？"
        android:textColor="#000000"
        android:textSize="40sp"
        android:id="@+id/f2_tv1"
        />

    <CheckBox
        android:id="@+id/f2_cb1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f2_tv1"
        android:text="饭"
        android:textColor="#000000"
        android:textSize="25sp"
        android:layout_marginTop="20dp"
        android:padding="10dp"
        />
    <CheckBox
        android:id="@+id/f2_cb2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f2_cb1"
        android:text="肉"
        android:textColor="#000000"
        android:textSize="25sp"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        />
    <CheckBox
        android:id="@+id/f2_cb3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f2_cb2"
        android:text="面"
        android:textColor="#000000"
        android:textSize="25sp"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        />
    <CheckBox
        android:id="@+id/f2_cb4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f2_cb3"
        android:text="菜"
        android:textColor="#000000"
        android:textSize="25sp"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        />
    <CheckBox
        android:id="@+id/f2_cb5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/f2_cb4"
        android:text="水果"
        android:textColor="#000000"
        android:textSize="25sp"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        />



</RelativeLayout>